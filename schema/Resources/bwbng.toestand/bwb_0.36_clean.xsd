<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:bwb-cms="http://schema.sdu.nl/bwb/cms/"
   elementFormDefault="qualified">

   <!-- 2013-09-04
      - toegevoegd: element afk
      - toegevoegd: element afk in groep tekst.maximaal
      - toegevoegd: element dossierref in element noot.al
      - toegevoegd: attribuut sleutel in element wetgeving
   -->

   <!-- 2013-11-27
      - ref naar attGroup bwbcms-attributen vanuit wetgeving vervangen door de 3 attributen
        reden: in toestand.xsd overschrijven we deze attGroup en voegen we @bwb-ng-variabel-deel toe
        dat moet voor alle elementen met een @label-id, behalve wetgeving
   -->

   <!-- 2014-09-08
      * ontbrekende attributen uit content toegevoegd:
      - toegevoegd: attribuut status toegevoegd op aanhef-, circulaire.aanhef-, intitule- en wetgeving-elementen;
      - toegevoegd: attribuut titel toegevoegd op afk-element;
      - toegevoegd: attributen alt en display op illustratie-element;
      - toegevoegd: attribuut summary toegevoegd op table-element.
      
      * ontbrekende elementen uit content toegevoegd:
      - toegevoegd: childelementen inf, nadruk, omissie, ovl, sup en unl toegevoegd aan citeertitel-element;
      - toegevoegd: childelement al-groep toegevoegd aan structuurtekst en wettekst.
   -->

   <!-- 2014-09-10
      - toegevoegd: attribuut urlidentifier aan publicatie-element adv een issue tfs31005;
      - aangepast: attribuut type op illustratie optioneel gemaakt.
   -->
   
   <!-- 2014-09-17
      - toegevoegd: elementen cit, dfn op tekst.algemeen;
      - toegevoegd: element eg-richtlijn op considerans.al;
      - toegevoegd: element uitgegeven op uitgifte;
      - toegevoegd: element artikelkop op wijziging.
   -->
   
   <!-- 2014-09-22
      - toegevoegd: publicatie toegevoegd op cms-attributen
   -->

   <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml_0.36.xsd"/>
   <xs:group name="lokaal.structuur.formeel">
      <xs:sequence>
         <xs:element minOccurs="0" ref="meta-data"/>
      </xs:sequence>
   </xs:group>

   <xs:element name="voorlopigetoepassing">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="publicatie" minOccurs="0"/>
            <xs:element name="voorlopigetoepassing.datum"/>
            <xs:element name="beschrijving-voorlopigetoepassing" minOccurs="0">
               <xs:complexType mixed="true">
                  <!-- 2013-05-08 bug in schema opgelost: zowel lokale als globale definitie van beschrijving-voorlopigetoepassing; copy al / lijst -->
                  <xs:choice maxOccurs="unbounded">
                     <xs:element ref="al"/>
                     <xs:element ref="lijst"/>
                  </xs:choice>
                  <xs:attribute name="isodatum"/>
               </xs:complexType>
            </xs:element>
         </xs:sequence>
      </xs:complexType>
   </xs:element>

   <!--   <xs:element name="beschrijving-voorlopigetoepassing">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded">
            <xs:element ref="al"/>
            <xs:element ref="lijst"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>-->

   <xs:attributeGroup name="bwbcms-attributen">
      <xs:attribute name="label-id"/>
      <xs:attribute name="stam-id"/>
      <xs:attribute name="versie-id"/>
      <xs:attribute name="publicatie">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="ja"/>
               <xs:enumeration value="nee"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>

   <xs:attributeGroup name="lokaal.att.wetsluiting">
      <xs:attribute name="status" default="goed">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="vervallen"/>
               <xs:enumeration value="goed"/>
               <xs:enumeration value="nogniet"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="bwbcms-attributen"/>
   </xs:attributeGroup>

   <xs:element name="lokaal.structuur.li" abstract="true">
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="title"/>
            <xs:element maxOccurs="unbounded" ref="tgroup"/>
            <xs:group ref="lokaal.tabeleind"/>
         </xs:sequence>
         <xs:attributeGroup ref="attlist.table"/>
      </xs:complexType>
   </xs:element>
   <xs:group name="lokaal.structuur.artikel">
      <xs:sequence>
         <xs:element ref="binnenwet"/>
      </xs:sequence>
   </xs:group>
   <xs:group name="lokaal.structuur.lid">
      <xs:sequence>
         <xs:element ref="binnenwet"/>
      </xs:sequence>
   </xs:group>
   <xs:group name="lokaal.tekst.al">
      <xs:choice>
         <xs:element ref="redactie"/>
         <xs:element ref="tekstcorrectie"/>
      </xs:choice>
   </xs:group>
   <xs:group name="lokaal.tekst.nr">
      <xs:choice>
         <xs:element ref="redactie"/>
         <xs:element ref="tekstcorrectie"/>
      </xs:choice>
   </xs:group>
   <xs:attributeGroup name="lokaal.att.nr">
      <xs:attribute name="status" default="officieel">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="officieel"/>
               <xs:enumeration value="redactioneel"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:group name="lokaal.tekst.titel">
      <xs:choice>
         <xs:element ref="redactie"/>
         <xs:element ref="tekstcorrectie"/>
      </xs:choice>
   </xs:group>
   <xs:group name="lokaal.structuur.divisie">
      <xs:choice>
         <xs:element ref="wetcitaat"/>
         <xs:element ref="model"/>
         <xs:element ref="artikel"/>
      </xs:choice>
   </xs:group>
   <xs:group name="lokaal.structuur.bijlage">
      <xs:choice>
         <xs:element ref="wetcitaat"/>
         <xs:element ref="model"/>
         <xs:element ref="officiele-inhoudsopgave"/>
         <xs:element ref="artikel"/>
      </xs:choice>
   </xs:group>
   <xs:group name="lokaal.tekst.datum">
      <xs:sequence>
         <xs:element ref="nadruk"/>
      </xs:sequence>
   </xs:group>
   <xs:attributeGroup name="lokaal.att.titel">
      <xs:attribute name="status" default="officieel">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="officieel"/>
               <xs:enumeration value="redactioneel"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:attributeGroup name="lokaal.att.artikel">
      <xs:attribute name="status" default="goed">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="nogniet"/>
               <xs:enumeration value="vervallen"/>
               <xs:enumeration value="goed"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="code"/>
      <xs:attribute name="type"/>
   </xs:attributeGroup>
   <xs:attributeGroup name="lokaal.att.inhoud">
      <xs:attribute name="status" default="goed">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="nogniet"/>
               <xs:enumeration value="vervallen"/>
               <xs:enumeration value="goed"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="code"/>
      <xs:attribute name="type"/>
   </xs:attributeGroup>
   <xs:element name="lokaal.structuur.wetgeving" abstract="true">
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="class.wijzig-artikel">
               <xs:attributeGroup ref="attlist.wijzig-artikel"/>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:group name="tekst.minimaal">
      <xs:choice>
         <xs:element ref="nadruk"/>
         <xs:element ref="sup"/>
         <xs:element ref="inf"/>
         <xs:element ref="ovl"/>
         <xs:element ref="unl"/>
         <xs:element ref="omissie"/>
      </xs:choice>
   </xs:group>
   <xs:group name="tekst.algemeen">
      <xs:choice>
         <xs:group ref="tekst.minimaal"/>
         <xs:element ref="nootref"/>
         <xs:element ref="noot"/>
         <xs:element ref="aanhaling"/>
      </xs:choice>
   </xs:group>
   <xs:group name="tekst.maximaal">
      <xs:choice>
         <xs:group ref="tekst.algemeen"/>
         <xs:element ref="intref"/>
         <xs:element ref="extref"/>
         <xs:element ref="extref-groep"/>
         <xs:element ref="intref-groep"/>
         <xs:element ref="dossierref"/>
         <xs:element ref="indexterm"/>
         <xs:element ref="afk"/>
         <xs:element ref="cit"/>
         <xs:element ref="dfn"/>
      </xs:choice>
   </xs:group>
   <xs:attributeGroup name="att.taal">
      <xs:attribute ref="xml:lang"/>
   </xs:attributeGroup>
   <xs:element name="aanhaling">
      <xs:complexType mixed="true">
         <xs:group minOccurs="0" maxOccurs="unbounded" ref="tekst.minimaal"/>
         <xs:attributeGroup ref="attlist.aanhaling"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.aanhaling">
      <xs:attribute name="bron"/>
      <xs:attribute name="url"/>
      <xs:attribute name="auteur"/>
      <xs:attributeGroup ref="att.taal"/>
   </xs:attributeGroup>
   <xs:element name="nadruk">
      <xs:complexType mixed="true">
         <xs:group minOccurs="0" maxOccurs="unbounded" ref="tekst.algemeen"/>
         <xs:attributeGroup ref="attlist.nadruk"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.nadruk">
      <xs:attribute name="type" default="cur">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="vet"/>
               <xs:enumeration value="cur"/>
               <xs:enumeration value="vetcur"/>
               <xs:enumeration value="rom"/>
               <xs:enumeration value="ondlijn"/>
               <xs:enumeration value="nonprop"/>
               <xs:enumeration value="klkap"/>
               <xs:enumeration value="kap"/>
               <xs:enumeration value="halfvet"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:element name="sup" type="xs:string"/>
   <xs:element name="inf" type="xs:string"/>
   <xs:element name="ovl" type="xs:string"/>
   <xs:element name="unl" type="xs:string"/>
   <xs:element name="indexterm">
      <xs:complexType mixed="true">
         <xs:group minOccurs="0" maxOccurs="unbounded" ref="tekst.algemeen"/>
         <xs:attributeGroup ref="attlist.indexterm"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.indexterm">
      <xs:attribute name="id"/>
      <xs:attribute name="term"/>
      <xs:attribute name="indexgroep"/>
   </xs:attributeGroup>
   <xs:element name="omissie">
      <xs:complexType>
         <xs:attributeGroup ref="attlist.omissie"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.omissie">
      <xs:attribute name="reden"/>
      <xs:attribute name="bron"/>
      <xs:attribute name="plaatsing" default="inline">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="voetnoot"/>
               <xs:enumeration value="inline"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
  <xs:element name="dfn">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="nadruk"/>
        <xs:element ref="sup"/>
        <xs:element ref="inf"/>
        <xs:element ref="ovl"/>
        <xs:element ref="unl"/>
        <xs:element ref="omissie"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="cit">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="nadruk"/>
        <xs:element ref="sup"/>
        <xs:element ref="inf"/>
        <xs:element ref="ovl"/>
        <xs:element ref="unl"/>
        <xs:element ref="omissie"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
   <xs:attributeGroup name="att.datum">
      <xs:attribute name="isodatum" use="required"/>
   </xs:attributeGroup>
   <xs:element name="structuur.minimaal" abstract="true" substitutionGroup="structuur.algemeen"/>
   <xs:element name="structuur.algemeen" abstract="true"/>
   <xs:group name="structuur.maximaal">
      <xs:choice>
         <xs:element ref="structuur.algemeen"/>
         <xs:element ref="table"/>
      </xs:choice>
   </xs:group>
   <xs:complexType name="class.al" mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
         <xs:group ref="tekst.maximaal"/>
         <xs:group ref="lokaal.tekst.al"/>
      </xs:choice>
   </xs:complexType>
   <xs:element name="al" substitutionGroup="structuur.minimaal">
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="class.al">
               <xs:attributeGroup ref="attlist.al"/>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.al">
      <xs:attribute name="id"/>
      <xs:attributeGroup ref="att.taal"/>
   </xs:attributeGroup>
   <xs:element name="al-groep">
      <xs:complexType>
         <xs:choice maxOccurs="unbounded">
            <xs:element ref="al"/>
            <xs:element ref="lijst"/>
         </xs:choice>
      </xs:complexType>
   </xs:element>
   <xs:element name="citaat" substitutionGroup="structuur.algemeen">
      <xs:complexType>
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="structuur.minimaal"/>
            <xs:element ref="plaatje"/>
            <xs:element ref="formule"/>
         </xs:choice>
         <xs:attributeGroup ref="attlist.citaat"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.citaat">
      <xs:attribute name="id"/>
      <xs:attribute name="bron"/>
      <xs:attribute name="url"/>
      <xs:attribute name="auteur"/>
      <xs:attributeGroup ref="att.taal"/>
   </xs:attributeGroup>
   <xs:element name="kop">
      <xs:complexType>
         <xs:choice>
            <xs:sequence>
               <xs:element ref="label"/>
               <xs:element minOccurs="0" ref="nr"/>
               <xs:element minOccurs="0" ref="label"/>
               <xs:element minOccurs="0" ref="titel"/>
               <xs:element minOccurs="0" ref="subtitel"/>
            </xs:sequence>
            <xs:sequence>
               <xs:element ref="nr"/>
               <xs:element minOccurs="0" ref="label"/>
               <xs:element minOccurs="0" ref="titel"/>
               <xs:element minOccurs="0" ref="subtitel"/>
            </xs:sequence>
            <xs:sequence>
               <xs:element ref="titel"/>
               <xs:element minOccurs="0" ref="subtitel"/>
            </xs:sequence>
         </xs:choice>
         <xs:attribute name="kopopmaak">
            <!-- 2013-05-08 toegevoegd omdat het in BWBR0030835 blijkt voor te komen en in bwbcms 1.61 wordt toegestaan-->
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="vet"/>
                  <xs:enumeration value="cur"/>
                  <xs:enumeration value="vetcur"/>
                  <xs:enumeration value="rom"/>
                  <xs:enumeration value="ondlijn"/>
                  <xs:enumeration value="nonprop"/>
                  <xs:enumeration value="klkap"/>
                  <xs:enumeration value="kap"/>
                  <xs:enumeration value="halfvet"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="class.titel" mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded">
         <xs:group ref="tekst.maximaal"/>
         <xs:group ref="lokaal.tekst.titel"/>
      </xs:choice>
   </xs:complexType>
   <xs:element name="titel">
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="class.titel">
               <xs:attributeGroup ref="attlist.titel"/>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.titel">
      <xs:attribute name="id"/>
      <xs:attributeGroup ref="lokaal.att.titel"/>
   </xs:attributeGroup>
   <xs:element name="subtitel" type="class.titel"/>
   <xs:element name="nr">
      <xs:complexType mixed="true">
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="noot"/>
            <xs:element ref="nootref"/>
            <xs:group ref="lokaal.tekst.nr"/>
         </xs:choice>
         <xs:attributeGroup ref="attlist.nr"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.nr">
      <xs:attribute name="id"/>
      <xs:attributeGroup ref="lokaal.att.nr"/>
   </xs:attributeGroup>
   <xs:element name="label" type="xs:string"/>
   <xs:element name="tussenkop" substitutionGroup="structuur.algemeen">
      <xs:complexType mixed="true">
         <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:group ref="tekst.maximaal"/>
            <xs:group ref="lokaal.tekst.titel"/>
         </xs:choice>
         <xs:attributeGroup ref="attlist.tussenkop"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.tussenkop">
      <xs:attribute name="id"/>
      <xs:attribute name="kopopmaak" default="cur">
         <xs:simpleType>
            <xs:restriction base="xs:string">
               <xs:enumeration value="vet"/>
               <xs:enumeration value="cur"/>
               <xs:enumeration value="vetcur"/>
               <xs:enumeration value="rom"/>
               <xs:enumeration value="ondlijn"/>
               <xs:enumeration value="nonprop"/>
               <xs:enumeration value="klkap"/>
               <xs:enumeration value="kap"/>
               <xs:enumeration value="halfvet"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>

   </xs:attributeGroup>
   <xs:element name="plaatje" substitutionGroup="structuur.algemeen">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="illustratie"/>
            <xs:element minOccurs="0" ref="bijschrift"/>
            <xs:element minOccurs="0" ref="bron"/>
         </xs:sequence>
         <xs:attributeGroup ref="attlist.plaatje"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.plaatje">
      <xs:attribute name="id"/>
      <xs:attributeGroup ref="att.taal"/>
   </xs:attributeGroup>
   <xs:element name="formule" substitutionGroup="structuur.algemeen">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="illustratie"/>
            <xs:element minOccurs="0" ref="bijschrift"/>
            <xs:element minOccurs="0" ref="bron"/>
         </xs:sequence>
         <xs:attributeGroup ref="attlist.formule"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.formule">
      <xs:attribute name="id"/>
      <xs:attributeGroup ref="att.taal"/>
   </xs:attributeGroup>
   <xs:element name="bijschrift">
      <xs:complexType mixed="true">
         <xs:group minOccurs="0" maxOccurs="unbounded" ref="tekst.algemeen"/>
         <xs:attributeGroup ref="attlist.bijschrift"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.bijschrift">
      <xs:attribute name="locatie" default="onder">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="boven"/>
               <xs:enumeration value="onder"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attributeGroup ref="att.taal"/>
   </xs:attributeGroup>
   <xs:element name="bron">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="al"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:complexType name="class.datum" mixed="true">
      <xs:group minOccurs="0" maxOccurs="unbounded" ref="lokaal.tekst.datum"/>
   </xs:complexType>
   <xs:element name="datum">
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="class.datum">
               <xs:attributeGroup ref="attlist.datum"/>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.datum">
      <xs:attributeGroup ref="att.datum"/>
   </xs:attributeGroup>
   <xs:attributeGroup name="att.lijst.type">
      <xs:attribute name="type" default="expliciet">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="1"/>
               <xs:enumeration value="a"/>
               <xs:enumeration value="A"/>
               <xs:enumeration value="I"/>
               <xs:enumeration value="i"/>
               <xs:enumeration value="bol"/>
               <xs:enumeration value="vierkant"/>
               <xs:enumeration value="cirkel"/>
               <xs:enumeration value="mdash"/>
               <xs:enumeration value="ndash"/>
               <xs:enumeration value="asterisk"/>
               <xs:enumeration value="ongemarkeerd"/>
               <xs:enumeration value="expliciet"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
   </xs:attributeGroup>
   <xs:element name="naamlijst">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="naam"/>
         </xs:sequence>
         <xs:attributeGroup ref="attlist.naamlijst"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.naamlijst">
      <xs:attribute name="id"/>
      <xs:attribute name="start" default="1" type="xs:NMTOKEN"/>
      <xs:attribute name="nr-sluiting" default="."/>
      <xs:attributeGroup ref="att.lijst.type"/>
      <xs:attributeGroup ref="att.taal"/>
   </xs:attributeGroup>
   <xs:element name="naam">
      <xs:complexType>
         <xs:sequence>
            <xs:element minOccurs="0" ref="voornaam"/>
            <xs:element ref="achternaam"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>
   <xs:element name="voornaam" type="xs:string"/>
   <xs:element name="achternaam" type="xs:string"/>
   <xs:element name="lijst" substitutionGroup="structuur.minimaal">
      <xs:complexType>
         <xs:sequence>
            <xs:element maxOccurs="unbounded" ref="li"/>
         </xs:sequence>
         <xs:attributeGroup ref="attlist.lijst"/>
      </xs:complexType>
   </xs:element>
   <xs:attributeGroup name="attlist.lijst">
      <xs:attributeGroup ref="att.lijst.type"/>
      <xs:attribute name="id"/>
      <xs:attribute name="start" default="1" type="xs:NMTOKEN"/>
      <xs:attribute name="level" default="single">
         <xs:simpleType>
            <xs:restriction base="xs:token">
               <xs:enumeration value="multiple"/>
               <xs:enumeration value="single"/>
            </xs:restriction>
         </xs:simpleType>
      </xs:attribute>
      <xs:attribute name="nr-sluiting" default="."/>
